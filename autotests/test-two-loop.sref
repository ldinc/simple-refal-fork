VarSetDifference {
  ( e.Set1-B t.Common e.Set1-E ) ( e.Set2-B t.Common e.Set2-E ) =
    e.Set1-B <VarSetDifference (e.Set1-E) ( e.Set2-B e.Set2-E )>;

  ( e.Set1 ) ( e.Set2 ) = e.Set1;
}

$ENTRY Go {
  =
    <VarSetDifference
     ((1 'tFn' 1 )
      (2 'tFn' 1 )
      (1 'tNext' 1 )
      (1 'eTail' 1 ))
     ((1 'tFn' 1 )
      (1 'tNext' 1 )
      (1 'eTail' 1 ))>;
}